		<param>
			<name>Output Delegate</name>
			<type>Delegate</type>
			<description></description>
		</param>
		<param>
			<name>Out</name>
			<type>Exec</type>
			<description></description>
		</param>
		<param>
			<name>Attribute</name>
			<type>Gameplay Attribute Structure</type>
			<description></description>
		</param>
		<param>
			<name>Delta Value</name>
			<type>Float</type>
			<description></description>
		</param>
		<param>
			<name>Event Tags</name>
			<type>Gameplay Tag Container Structure</type>
			<description></description>
		</param>
	</outputs>
